<div class="container" style="margin-bottom: 100px;">
	<h3 class="my-5">Available cards</h3>

	<div class="my-5">
		<%= render 'shared/filter' %>
	</div>

	<div class="cards pb-5">
	  <% @cards.each do |card| %>
	    <%= render 'shared/card', card: card %>
	  <% end %>
	</div>

</div>

<%= render 'shared/selectorbar' %>
<%= render 'shared/confirmation', booking: @booking %>
